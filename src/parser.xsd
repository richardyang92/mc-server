<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.whut.mc.com/parser"
           xmlns="http://www.whut.mc.com/parser" elementFormDefault="qualified" attributeFormDefault="qualified">
    <xs:complexType name="parser">
        <xs:complexContent>
            <xs:extension base="xs:string">
                <xs:sequence>
                    <xs:element name="head" type="head" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="resolver" type="resolver" minOccurs="1" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="head">
        <xs:complexContent>
            <xs:extension base="xs:string">
                <xs:sequence>
                    <xs:element name="appId" type="xs:int" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="trentId" type="xs:int" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="frmId" type="xs:int" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="encrypt" type="xs:string" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="totalLen" type="xs:int" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="cs" type="xs:int" minOccurs="1" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="resolver">
        <xs:complexContent>
            <xs:extension base="xs:string">
                <xs:attribute name="name" type="xs:string"/>
                <xs:attribute name="type" type="xs:string"/>
                <xs:attribute name="regx" type="xs:string"/>
                <xs:attribute name="class" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="parser" type="parser"/>
</xs:schema>